package Constructor;

public class Employee {

	int ustId;
	String name;
	String location;
	double salary;
	
	//default constructor
	public Employee() {
		System.out.println("welcome to UST");
	}
	void display() {
		System.out.println("my Name"+name+"Working in UST with USTID:- "+ustId+"in Location "+location+"with salary "+salary);
	}
	
	public static void main(String[] args) {
		Employee e=new Employee();
		e.display();
	}
}
